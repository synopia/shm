<html>
<body>
<h1>SHM 2013 Einzelwertung</h1>

<table>
  <tr>
    <th>Mannschaft</th>
    <th>Turner</th>
    <th>Klasse</th>
    <th>Boden</th>
    <th>Pauschenpferd</th>
    <th>Ringe</th>
    <th>Sprung</th>
    <th>Barren</th>
    <th>Reck</th>
    <th>Punkte</th>
    <th>Platz</th>
  </tr>
  <% turners.each do |turner| %>
    <tr>
      <td><%= turner[:uni] %> <%= turner[:mannschaft] %></td>
      <td><%= turner[:name] %></td>
      <td><%= turner[:km] %></td>
      <td><%= turner[:id] %></td>
    </tr>
  <% end %>
</table>

</body>
</html>